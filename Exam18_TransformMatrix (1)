import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        int[][] arr = new int[N][N];
        for(int j=N-1;j>=0;j--) {
            for(int i=0;i<N;i++) {
                arr[i][j] = sc.nextInt();
            }
        }
        for(int i=0;i<N;i++) {
            for(int j =0;j<N;j++) {
                System.out.print(arr[i][j] + " ");
            }
            System.out.println();
        }
    }
}